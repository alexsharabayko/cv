<figure class="avatar">
    <figcaption class="avatar-title">{{=it.commonInfo.fullName}}</figcaption>

    <img src="{{=it.commonInfo.avatarSrc}}" alt="avatar"/>
</figure>

<ul class="main-nav">
    <li class="main-nav-li">
        <a href="/" class="main-nav-item fn-nav-item active">
            <i class="fa fa-user"></i>
            <span>About</span>
        </a>
    </li>
    <li class="main-nav-li">
        <a href="/skills" class="main-nav-item fn-nav-item">
            <i class="fa fa-gears"></i>
            <span>Skills</span>
        </a>
    </li>
    <li class="main-nav-li">
        <a href="/portfolio" class="main-nav-item fn-nav-item">
            <i class="fa fa-folder"></i>
            <span>Portfolio</span>
        </a>
    </li>
    <li class="main-nav-li">
        <a href="/contact" class="main-nav-item fn-nav-item">
            <i class="fa fa-phone"></i>
            <span>Contact</span>
        </a>
    </li>
</ul>

<ul class="social-links">
    {{~it.socialLinks:link}}
    <li class="social-links-li">
        <a href="{{=link.href}}" target="_blank"><i class="fa fa-{{=link.icon}}"></i></a>
    </li>
    {{~}}
</ul>